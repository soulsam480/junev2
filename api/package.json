{
  "name": "api",
  "version": "0.0.0",
  "description": "Junev2 api",
  "author": "soulsam480 <soulsam480@hotmail.com>",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --files -- src/index.ts",
    "build": "rm -rf dist && yarn && tsc && tsc-alias",
    "build:swagger": "node swagger.js",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir ./types && tsc-alias",
    "start": "cd dist && PORT=3002 PROD=true node index.js",
    "types": "tsc --noEmit"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^15.12.2",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth2": "^0.1.4",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1"
  },
  "dependencies": {
    "@typegoose/typegoose": "^8.2.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dango-core": "^1.5.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.13.8",
    "passport": "^0.4.1",
    "passport-google-oauth2": "^0.2.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "tsc-alias": "^1.3.8"
  },
  "workspaces": {
    "nohoist": [
      "**/mongoose/**"
    ]
  }
}
