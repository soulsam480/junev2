{
  "name": "@soulsam480/june",
  "version": "0.1.0",
  "description": "June component library",
  "scripts": {
    "dts": "tsc --declaration --emitDeclarationOnly --declarationDir ./dist --removeComments false",
    "build": "rm -rf dist && vite build && npm run dts"
  },
  "main": "./dist/june.cjs.js",
  "module": "./dist/june.es.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./june.es.js",
      "require": "./june.cjs.js",
      "types": "./june.d.ts"
    }
  },
  "unpkg": "./june.iife.js",
  "jsdelivr": "./june.iife.js",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.2",
    "@iconify/iconify": "latest",
    "@iconify/json": "latest",
    "vite-plugin-purge-icons": "latest"
  },
  "peerDependenciesMeta": {
    "@iconify/iconify": {
      "optional": true
    },
    "@iconify/json": {
      "optional": true
    },
    "vite-plugin-purge-icons": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "vite": "^2.7.4"
  },
  "sideEffect": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulsam480/junev2.git",
    "directory": "lib"
  },
  "keywords": [
    "june",
    "component-library",
    "react"
  ],
  "author": "soulsam480 <soulsam480@hotmail.com>",
  "private": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soulsam480/junev2/issues"
  },
  "homepage": "https://github.com/soulsam480/junev2#readme",
  "dependencies": {
    "clsx": "^1.1.1"
  }
}
