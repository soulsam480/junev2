#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/zoro/projects/personal/junev2/node_modules/.pnpm/vite@2.7.4/node_modules/vite/bin/node_modules:/home/zoro/projects/personal/junev2/node_modules/.pnpm/vite@2.7.4/node_modules/vite/node_modules:/home/zoro/projects/personal/junev2/node_modules/.pnpm/vite@2.7.4/node_modules:/home/zoro/projects/personal/junev2/node_modules/.pnpm/node_modules:/home/zoro/projects/personal/junev2/node_modules:/home/zoro/projects/personal/node_modules:/home/zoro/projects/node_modules:/home/zoro/node_modules:/home/node_modules:/node_modules:/home/zoro/projects/personal/junev2/lib/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/zoro/projects/personal/junev2/node_modules/.pnpm/vite@2.7.4/node_modules/vite/bin/node_modules:/home/zoro/projects/personal/junev2/node_modules/.pnpm/vite@2.7.4/node_modules/vite/node_modules:/home/zoro/projects/personal/junev2/node_modules/.pnpm/vite@2.7.4/node_modules:/home/zoro/projects/personal/junev2/node_modules/.pnpm/node_modules:/home/zoro/projects/personal/junev2/node_modules:/home/zoro/projects/personal/node_modules:/home/zoro/projects/node_modules:/home/zoro/node_modules:/home/node_modules:/node_modules:/home/zoro/projects/personal/junev2/lib/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vite@2.7.4/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vite@2.7.4/node_modules/vite/bin/vite.js" "$@"
fi
